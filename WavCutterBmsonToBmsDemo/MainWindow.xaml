<Window x:Class="WavCutterBmsonToBmsDemo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WavCutterBmsonToBmsDemo"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="400">
    <Grid>
        <Button x:Name="OutputButton" Content="Output" HorizontalAlignment="Left" Margin="35,179,0,0" VerticalAlignment="Top" Height="66" Width="125" Click="OutputButton_Click"/>
        <ComboBox x:Name="WavComboBox" HorizontalAlignment="Left" Margin="37,100,0,0" VerticalAlignment="Top" Width="306" SelectedValuePath="ID" DisplayMemberPath="Name"/>
        <Button x:Name="FileOpenButton" Content="File open" HorizontalAlignment="Left" Margin="37,29,0,0" VerticalAlignment="Top" Height="43" Width="306" Click="FileOpenButton_Click"/>
        <Button x:Name="OutputAllButton" Content="Output all" HorizontalAlignment="Left" Margin="219,179,0,0" VerticalAlignment="Top" Height="66" Width="124" Click="OutputAllButton_Click"/>

    </Grid>
</Window>
